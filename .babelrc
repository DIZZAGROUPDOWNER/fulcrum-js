{
  "presets": [
    ["env", {
      "targets": {
        "node": "6.10",
        "browsers": ["last 2 versions"]
      }
    }]
  ],
  "plugins": [
    "transform-async-to-generator",
    "add-module-exports",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
